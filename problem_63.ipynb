    "list_1 = [x for x in range(2,10)]\n",
    "sum=1 # 1**1 added\n",
    "for i in list_1:\n",
    "    a=0\n",
    "    while True:\n",
    "        a+=1\n",
    "        str_i = str(i**a)\n",
    "        if len(str_i)==a:\n",
    "            sum+=1\n",
    "        elif len(str_i)<a:\n",
    "            break\n",
    "print(sum)\n",
    "\n",
    "\n"
   ]
